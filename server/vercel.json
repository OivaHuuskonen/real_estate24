{
    "version": 2,
    "builds": [
      {
        "src": "dist/**",
        "use": "@vercel/static"
      },
      {
        "src": "/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  